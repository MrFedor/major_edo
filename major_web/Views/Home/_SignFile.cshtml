@model major_data.Models.FileInSystem
<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Подписать файл</h4>
    </div>
    <div class="modal-body">
        <div>
            @Html.HiddenFor(a => a.Id)
            <table class="table table-responsive">
                <tr>
                    <td>@Html.DisplayNameFor(a => a.RuleSystem.Dogovor.Client.Name)</td>
                    <td>@Html.DisplayFor(a => a.RuleSystem.Dogovor.Client.Name)</td>
                </tr>
                <tr>
                    <td>@Html.DisplayNameFor(a => a.RuleSystem.Fond.Name)</td>
                    <td>@Html.DisplayFor(a => a.RuleSystem.Fond.Name)</td>
                </tr>
                <tr>
                    <td>@Html.DisplayNameFor(a => a.Name)</td>
                    <td>@Html.DisplayFor(a => a.Name)</td>
                </tr>
                <tr>
                    <td>@Html.DisplayNameFor(a => a.DataCreate)</td>
                    <td>@Html.DisplayFor(a => a.DataCreate)</td>
                </tr>
                <tr>
                    <td>@Html.DisplayNameFor(a => a.CBInfo.TypeXML.Xml_type)</td>
                    <td>@Html.DisplayFor(a => a.CBInfo.TypeXML.Xml_type)</td>
                </tr>
                <tr>
                    <td>@Html.DisplayNameFor(a => a.CBInfo.PeriodXML)</td>
                    <td>@Html.DisplayFor(a => a.CBInfo.PeriodXML)</td>
                </tr>
            </table>
        </div>        
    </div>
    <div class="modal-footer">
        <button class="btn btn-default button_sign_cb" name="button_sign_cb" type="button">
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Подписать файл
        </button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Отмена</button>
    </div>
</div>


